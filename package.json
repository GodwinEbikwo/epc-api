{
  "name": "boiler-epc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "pnpm --dir backend dev",
    "build": "pnpm build:frontend && pnpm build:backend",
    "build:frontend": "next build",
    "build:backend": "pnpm --dir backend build",
    "start": "concurrently \"pnpm start:frontend\" \"pnpm start:backend\"",
    "start:frontend": "next start",
    "start:backend": "pnpm --dir backend start",
    "lint": "pnpm lint:frontend && pnpm lint:backend",
    "lint:frontend": "next lint",
    "lint:backend": "pnpm --dir backend lint",
    "install:backend": "pnpm --dir backend install",
    "clean": "rm -rf .next backend/node_modules node_modules"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.85.5",
    "@trpc/client": "^11.5.0",
    "@trpc/react-query": "^11.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.540.0",
    "next": "15.4.7",
    "nuqs": "^2.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.7",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
